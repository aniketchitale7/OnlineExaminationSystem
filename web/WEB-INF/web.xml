<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>dlogin</servlet-name>
        <servlet-class>olexam.dlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>olexam.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>queadd</servlet-name>
        <servlet-class>olexam.queadd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delque</servlet-name>
        <servlet-class>olexam.delque</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userdel</servlet-name>
        <servlet-class>olexam.userdel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contactus</servlet-name>
        <servlet-class>olexam.contactus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changepass</servlet-name>
        <servlet-class>olexam.changepassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userans1</servlet-name>
        <servlet-class>olexam.ansuser1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>useranscplus</servlet-name>
        <servlet-class>olexam.ansusercplus1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>useransclang</servlet-name>
        <servlet-class>olexam.ansuserclang1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dlogin</servlet-name>
        <url-pattern>/dlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>queadd</servlet-name>
        <url-pattern>/queadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delque</servlet-name>
        <url-pattern>/delque</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userdel</servlet-name>
        <url-pattern>/userdel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contactus</servlet-name>
        <url-pattern>/contactus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changepass</servlet-name>
        <url-pattern>/changepass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userans1</servlet-name>
        <url-pattern>/userans1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>useranscplus</servlet-name>
        <url-pattern>/useranscplus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>useransclang</servlet-name>
        <url-pattern>/useransclang</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
